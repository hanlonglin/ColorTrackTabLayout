<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorTrackView">
        <attr name="ct_changeColor" format="color" />
        <attr name="ct_progress" format="float" />
    </declare-styleable>

    <declare-styleable name="ColorTrackTabLayout">
        <attr name="ct_tabTextColor" format="color" />
        <attr name="ct_tabSelectTextColor" format="color" />
        <attr name="ct_tabTextSize" format="dimension" />
        <attr name="ct_tabSelectScale" format="float" />
        <attr name="ct_tabPadding" format="dimension" />
        <attr name="ct_startScrollX" format="dimension" />
        <attr name="ct_tabForeground" format="reference"/>
        <attr name="ct_tabMode">
            <enum name="scroll" value="0" />
            <enum name="fill" value="1" />
        </attr>
    </declare-styleable>
</resources>